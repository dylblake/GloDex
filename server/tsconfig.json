{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./",
    "types": ["node"]
  },
  "include": ["src/**/*.ts", "knexfile.ts", "migrations/**/*.ts", "seeds/**/*.ts"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
